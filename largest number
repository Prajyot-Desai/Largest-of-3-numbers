#include<iostream>
using namespace std;
int main()
{
    int a,b,c;
    cout<<"Enter the numbers: "<<endl;
    cin>>a>>b>>c;
    if(a>b && a>c)
    {
        cout<<"a is largest";
    }
    else if(a == b || a == c)
    {
        cout<<"You cant write same numbers";
    }
    else if(b>a && b>c)
    {
        cout<<"b is largest";
    }
    else if(b == a || b == c)
    {
        cout<<"You cant write same numbers";
    }
    else if(c>a && c>b)
    {
        cout<<"c is largest";
    }
    else if(c == b || c == a)
    {
        cout<<"You cant write same numbers";
    }
}
